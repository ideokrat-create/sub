<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>–ê–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏—è QlikView</title>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
            background: #ffffff;
            color: #000000;
            padding: 20px;
            text-align: center;
        }
        
        .container {
            max-width: 400px;
            margin: 0 auto;
            padding: 20px;
        }
        
        .status {
            padding: 15px;
            border-radius: 8px;
            margin: 20px 0;
            font-size: 14px;
            background: #e3f2fd;
            color: #1976d2;
        }
    </style>
</head>
<body>
    <div class="container">
        <h1>üîê –ê–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏—è</h1>
        <div id="status" class="status">–ò–¥–µ—Ç –∞–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏—è...</div>
    </div>

    <script>
        // –õ–æ–≥–∏–Ω –∏ –ø–∞—Ä–æ–ª—å –¥–ª—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–π –ø–æ–¥—Å—Ç–∞–Ω–æ–≤–∫–∏
        const LOGIN = 'test_mobile';
        const PASSWORD = 'test_m';
        const FORM_LOGIN_URL = 'https://report.subjoy.ru/qlikview/FormLogin.htm';
        
        function autoLogin() {
            try {
                // –ü–æ–ª—É—á–∞–µ–º URL –∏–∑ –ø–∞—Ä–∞–º–µ—Ç—Ä–∞ url
                const params = new URLSearchParams(window.location.search);
                const targetUrl = params.get('url') || params.get('back');
                
                if (!targetUrl) {
                    document.getElementById('status').textContent = '‚ùå –û—à–∏–±–∫–∞: –Ω–µ —É–∫–∞–∑–∞–Ω URL –¥–ª—è –ø–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è.';
                    document.getElementById('status').style.background = '#ffebee';
                    document.getElementById('status').style.color = '#c62828';
                    return;
                }
                
                // –î–µ–∫–æ–¥–∏—Ä—É–µ–º URL, –µ—Å–ª–∏ –æ–Ω –±—ã–ª –∑–∞–∫–æ–¥–∏—Ä–æ–≤–∞–Ω
                const decodedUrl = decodeURIComponent(targetUrl);
                
                // –ò—Å–ø–æ–ª—å–∑—É–µ–º fetch API –≤–º–µ—Å—Ç–æ form.submit() –¥–ª—è –∏–∑–±–µ–∂–∞–Ω–∏—è ERR_HTTP2_PROTOCOL_ERROR
                // –°–æ–∑–¥–∞–µ–º FormData –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ POST –∑–∞–ø—Ä–æ—Å–∞
                const formData = new FormData();
                formData.append('back', decodedUrl);
                formData.append('username', LOGIN);
                formData.append('password', PASSWORD);
                // –¢–∞–∫–∂–µ –¥–æ–±–∞–≤–ª—è–µ–º –∞–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–µ –∏–º–µ–Ω–∞ –ø–æ–ª–µ–π –Ω–∞ —Å–ª—É—á–∞–π, –µ—Å–ª–∏ –æ–Ω–∏ –Ω—É–∂–Ω—ã
                formData.append('user', LOGIN);
                formData.append('pwd', PASSWORD);
                
                // –û—Ç–ø—Ä–∞–≤–ª—è–µ–º POST –∑–∞–ø—Ä–æ—Å —á–µ—Ä–µ–∑ fetch API –¥–ª—è –∏–∑–±–µ–∂–∞–Ω–∏—è –ø—Ä–æ–±–ª–µ–º —Å HTTP/2
                setTimeout(async () => {
                    try {
                        // –û—Ç–ø—Ä–∞–≤–ª—è–µ–º POST –∑–∞–ø—Ä–æ—Å
                        const response = await fetch(FORM_LOGIN_URL, {
                            method: 'POST',
                            body: formData,
                            credentials: 'include',
                            redirect: 'follow',
                            mode: 'cors'
                        });
                        
                        // –ü–æ—Å–ª–µ —É—Å–ø–µ—à–Ω–æ–π –æ—Ç–ø—Ä–∞–≤–∫–∏ –ø–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª—è–µ–º –Ω–∞ –æ—Ç–≤–µ—Ç —Å–µ—Ä–≤–µ—Ä–∞
                        // –ï—Å–ª–∏ —Å–µ—Ä–≤–µ—Ä –≤–µ—Ä–Ω—É–ª —Ä–µ–¥–∏—Ä–µ–∫—Ç, –æ–Ω –±—É–¥–µ—Ç –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –æ–±—Ä–∞–±–æ—Ç–∞–Ω
                        if (response.ok || response.redirected) {
                            window.location.href = response.url || decodedUrl;
                        } else {
                            // –ï—Å–ª–∏ fetch –Ω–µ —Å—Ä–∞–±–æ—Ç–∞–ª –∏–∑-–∑–∞ CORS, –∏—Å–ø–æ–ª—å–∑—É–µ–º fallback –º–µ—Ç–æ–¥
                            submitFormFallback(decodedUrl);
                        }
                    } catch (error) {
                        console.error('–û—à–∏–±–∫–∞ –ø—Ä–∏ –æ—Ç–ø—Ä–∞–≤–∫–µ —á–µ—Ä–µ–∑ fetch:', error);
                        // –ï—Å–ª–∏ fetch –Ω–µ —Ä–∞–±–æ—Ç–∞–µ—Ç, –∏—Å–ø–æ–ª—å–∑—É–µ–º fallback –º–µ—Ç–æ–¥ —Å iframe
                        submitFormFallback(decodedUrl);
                    }
                }, 1500);
                
                // Fallback –º–µ—Ç–æ–¥: —Å–æ–∑–¥–∞–Ω–∏–µ —Ñ–æ—Ä–º—ã –∏ –æ—Ç–ø—Ä–∞–≤–∫–∞ —á–µ—Ä–µ–∑ iframe –¥–ª—è –æ–±—Ö–æ–¥–∞ HTTP/2 –ø—Ä–æ–±–ª–µ–º
                function submitFormFallback(decodedUrl) {
                    try {
                        const form = document.createElement('form');
                        form.method = 'POST';
                        form.action = FORM_LOGIN_URL;
                        form.style.display = 'none';
                        
                        // –°–æ–∑–¥–∞–µ–º iframe –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —Ñ–æ—Ä–º—ã
                        const iframe = document.createElement('iframe');
                        iframe.name = 'formTarget';
                        iframe.style.display = 'none';
                        document.body.appendChild(iframe);
                        
                        form.target = 'formTarget';
                        
                        const backInput = document.createElement('input');
                        backInput.type = 'hidden';
                        backInput.name = 'back';
                        backInput.value = decodedUrl;
                        form.appendChild(backInput);
                        
                        const usernameInput = document.createElement('input');
                        usernameInput.type = 'text';
                        usernameInput.name = 'username';
                        usernameInput.value = LOGIN;
                        form.appendChild(usernameInput);
                        
                        const userInput = document.createElement('input');
                        userInput.type = 'text';
                        userInput.name = 'user';
                        userInput.value = LOGIN;
                        form.appendChild(userInput);
                        
                        const passwordInput = document.createElement('input');
                        passwordInput.type = 'password';
                        passwordInput.name = 'password';
                        passwordInput.value = PASSWORD;
                        form.appendChild(passwordInput);
                        
                        const pwdInput = document.createElement('input');
                        pwdInput.type = 'password';
                        pwdInput.name = 'pwd';
                        pwdInput.value = PASSWORD;
                        form.appendChild(pwdInput);
                        
                        document.body.appendChild(form);
                        
                        // –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —Ñ–æ—Ä–º—É
                        form.submit();
                        
                        // –ü–æ—Å–ª–µ –Ω–µ–±–æ–ª—å—à–æ–π –∑–∞–¥–µ—Ä–∂–∫–∏ –ø–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª—è–µ–º –Ω–∞ —Ü–µ–ª–µ–≤–æ–π URL
                        setTimeout(() => {
                            window.location.href = decodedUrl;
                        }, 2000);
                    } catch (error) {
                        console.error('–û—à–∏–±–∫–∞ –ø—Ä–∏ –æ—Ç–ø—Ä–∞–≤–∫–µ —Ñ–æ—Ä–º—ã fallback:', error);
                        document.getElementById('status').textContent = `‚ùå –û—à–∏–±–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏ —Ñ–æ—Ä–º—ã: ${error.message}. –ü—Ä–æ–≤–µ—Ä—å—Ç–µ —Å–µ—Ä—Ç–∏—Ñ–∏–∫–∞—Ç —Å–∞–π—Ç–∞ –∏ –ø–æ–ø—Ä–æ–±—É–π—Ç–µ –æ—Ç–∫—Ä—ã—Ç—å —Å—Å—ã–ª–∫—É –≤—Ä—É—á–Ω—É—é.`;
                        document.getElementById('status').style.background = '#ffebee';
                        document.getElementById('status').style.color = '#c62828';
                    }
                }
                
            } catch (e) {
                document.getElementById('status').textContent = `‚ùå –û—à–∏–±–∫–∞: ${e.message}`;
                document.getElementById('status').style.background = '#ffebee';
                document.getElementById('status').style.color = '#c62828';
            }
        }
        
        // –ó–∞–ø—É—Å–∫–∞–µ–º –∞–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏—é –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ —Å—Ç—Ä–∞–Ω–∏—Ü—ã
        window.addEventListener('load', function() {
            autoLogin();
        });
    </script>
</body>
</html>

